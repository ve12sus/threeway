<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <div class="site-container">

      <header class="site-header">
        <!-- site navigation -->
        <squarespace:navigation navigationId="siteNav" template="site-navigation" />

      </header>

      <main class="content-container" role="main" data-content-field="main-content">
        <div class="container-fluid">
          <div id="carouselExampleIndicators" class="carousel slide" data-interval="false" data-ride="carousel">
            <ol class="carousel-indicators">
              <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
              <div class="caption-wrapper">
              <squarespace:query collection="carousel-images" featured="true">
              {.section items}
                {.section 0}
                <div id="caption" class="container">
                  <div class="caption">
                  {.excerpt?}
                    {.section excerpt}
                    <h1>{@|safe}</h1>
                    {.end}
                  {.or}
                    <h1>WELCOME TO OUR NEW<br>WEBSITE.</h1>
                  {.end}
                    <p class="welcome-readmore">
                  {.section sourceUrl}
                      <a class="btn" href="{@}" role="button">READ MORE</a>
                  {.or}
                      <a class="btn" href="/news" role="button">READ MORE</a>
                  {.end}
                    </p>
                  </div>
                </div>
                {.end}
              {.end}
              </squarespace:query>
              </div>
              <squarespace:query collection="carousel-images">
              {.section items}
              <div class="carousel-item active">
                {.section 0}
                <img class="d-block w-100" src="{assetUrl}" alt="First slide">
                {.end}
              </div>
              <div class="carousel-item">
                {.section 1}
                <img class="d-block w-100" src="{assetUrl}" alt="Second slide">
                {.end}
              </div>
              <div class="carousel-item">
                {.section 2}
                <img class="d-block w-100" src="{assetUrl}" alt="Third slide">
                {.end}
              </div>
              {.end}
              </squarespace:query>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>
          
          <div id="main" class="container">
            <div class="featured-posts">
              <div class="row no-gutters">
                <div class="col-md">
                  <div class="card text-white text-center border-0">
                  <squarespace:query collection="header-images" category="news" limit="1">
                  {.section items}
                    {.section 0}
                    {.main-image?}
                    <a href="/news" style="display:block;"><img class="card-img h-100" src="{assetUrl}?format=500w" alt="Card image"></a>
                    {.end}
                    <a href="/news" style="display:block;">
                      <div class="card-img-overlay d-flex featured-news">
                        <a href="/news" class="btn">READ MORE</a>
                        <div class="featured-overlay-content my-auto mx-auto">
                          <h2 class="card-title">NEWS</h2>
                          <p class="card-text large">Local stories, promotions, and sponsorships</p>
                        </div>
                    {.end}
                  {.end}
                  </squarespace:query>
                      </div>
                    </a>
                  </div>
                </div>
                <div class="col-md">
                  <div class="card text-white text-center border-0">
                  <squarespace:query collection="header-images" category="food" limit="1">
                  {.section items}
                    {.section 0}
                    {.main-image?}
                    <img class="card-img h-100" src="{assetUrl}?format=500w" alt="Card image">
                    {.end}
                    <div class="card-img-overlay d-flex featured-food">
                      <a href="/food" class="btn">READ MORE</a>
                      <div class="featured-overlay-content my-auto mx-auto">
                        <h2 class="card-title">FOOD</h2>
                        <p class="card-text large">Explore our new products and nutrition facts</p>
                      </div>
                    {.end}
                  {.end}
                  </squarespace:query>
                    </div>
                  </div>
                </div>
              </div>
              <div class="row no-gutters justify-content-center">
                <div class="col-md">
                  <div class="card text-white text-center border-0">
                  <squarespace:query collection="header-images" category="people" limit="1">
                  {.section items}
                    {.section 0}
                    {.main-image?}
                    <img class="card-img h-100" src="{assetUrl}?format=500w" alt="Card image">
                    {.end}
                    <div class="card-img-overlay d-flex featured-people">
                      <a href="/people" class="btn">READ MORE</a>
                      <div class="featured-overlay-content my-auto mx-auto">
                        <h2 class="card-title">PEOPLE</h2>
                        <p class="card-text large ">Meet our crew</p>
                      </div>
                    {.end}
                  {.end}
                  </squarespace:query>
                    </div>
                  </div>
                </div>
                <div class="col-md">
                  <div class="card text-white text-center border-0">
                  <squarespace:query collection="header-images" category="events" limit="1">
                  {.section items}
                    {.section 0}
                    {.main-image?}
                    <img class="card-img h-100" src="{assetUrl}?format=500w" alt="Card image">
                    {.end}
                    <div class="card-img-overlay d-flex featured-events">
                      <a href="/events" class="btn">READ MORE</a>
                      <div class="featured-overlay-content my-auto mx-auto">
                        <h2 class="card-title">EVENTS</h2>
                        <p class="card-text large">Local events near you</p>
                      </div>
                    {.end}
                  {.end}
                  </squarespace:query>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="social-links">
              <div class="row no-gutters ub-eats">
                <div class="col-12 col-md-4 ub-img">
                   <img src="../assets/img-mcd@2x.png">
                </div>
                <div class="col-12 col-md-8">
                  <div class="ub-content">
                    <h2>MCDELIVERY</h2>
                    <p class="large">Order your McDonald's faves on UberEATS</p>
                    <a href="https://www.mcdonalds.com/us/en-us/mobile-order-and-pay.html" class="btn" role="button">DOWNLOAD THE APP</a>
                  </div>
                </div>
              </div>
              <div class="row no-gutters ad-2-10 rmhc">
                <div class="col-12 col-md-2 logo">
                  <img src="assets/logo-rmhc@2x.png">
                </div>
                <div class="col-12 col-md-10 content-wrapper">
                  <div class="content">
                    <h2>Baby Everett needed surgery 1,000 miles from home</h2>
                    <a href="http://support.rmhc.org/site/PageNavigator/pw/Donation_Landing.html" class="btn">READ MORE</a>
                  </div>
                </div>
                <div class="overlay"></div>
              </div>
              <div id="carousel-ig" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                  <li data-target="#carousel-ig" data-slide-to="0" class="active"></li>
                  <li data-target="#carousel-ig" data-slide-to="1"></li>
                  <li data-target="#carousel-ig" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner">
                  <div class="carousel-item active">
                    <img class="d-block w-100" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/e15/23594243_401385300275853_5632111106257321984_n.jpg" alt="First slide">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block w-100" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/s640x640/sh0.08/e35/22427212_904192256411988_8262467353137119232_n.jpg" alt="Second slide">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                  <div class="carousel-item">
                    <img class="d-block w-100" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/s640x640/sh0.08/e35/22352233_1954233508161087_5643145057629372416_n.jpg" alt="Third slide">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                </div>
                <a class="carousel-control-prev" href="#carousel-ig" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carousel-ig" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>
              <div class="card-group">
                <div class="card text-white d-none d-md-block">
                  <img class="card-img h-100" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/e15/23594243_401385300275853_5632111106257321984_n.jpg" alt="Card image">
                  <div class="card-img-overlay">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                </div>
                <div class="card text-white d-none d-md-block">
                  <img class="card-img img-fluid" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/s640x640/sh0.08/e35/22427212_904192256411988_8262467353137119232_n.jpg" alt="Card image">
                  <div class="card-img-overlay">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                </div>
                <div class="card text-white d-none d-md-block">
                  <img class="card-img" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/s640x640/sh0.08/e35/22352233_1954233508161087_5643145057629372416_n.jpg" alt="Card image">
                  <div class="card-img-overlay">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                </div>
                <div class="card text-white d-none d-md-block">
                  <img class="card-img" src="https://scontent-lga3-1.cdninstagram.com/t51.2885-15/s640x640/sh0.08/e35/22220962_372140143215971_6024339098959872000_n.jpg" alt="Card image">
                  <div class="card-img-overlay">
                    <img src="assets/ic-instagram-on.png"
                        srcset="assets/ic-instagram-on@2x.png 2x,
                        assets/ic-instagram-on@3x.png 3x"
                        class="ic-instagram-on">
                  </div>
                </div>
              </div>
              <div class="card-group">
                <div class="card twitter d-none d-md-block">
                  <img src="assets/ic-twitter-on.png"
                  		 srcset="assets/ic-twitter-on@2x.png 2x,
                  					 assets/ic-twitter-on@3x.png 3x"
                  		 class="ic-twitter-on">
                  <img class="card-img-top" src="/assets/frap.jpg" alt="Card image cap">
                  <div class="card-body">
                    <h4 class="card-title">@McDonalds</h4>
                    <p class="card-text regular">Me: I'm going to start decorating right now.<br>Also me: Not before this <span class="hashtag">#McCafe</span> Peppermint Mocha.</p>
                  </div>
                </div>
                <div class="card twitter d-none d-md-block">
                  <img src="assets/ic-twitter-on.png"
                       srcset="assets/ic-twitter-on@2x.png 2x,
                             assets/ic-twitter-on@3x.png 3x"
                       class="ic-twitter-on">
                  <img class="card-img-top" src="/assets/kitchen.jpg" alt="Card image cap">
                  <div class="card-body">
                    <h4 class="card-title">@McDonalds</h4>
                    <p class="card-text regular">Our annual <span class="hashtag">@RMHC</span> fundraiser may be over but you can still donate your time. RMHC is always looking for helping hands. rmhc.org/chapter-search</p>
                  </div>
                </div>
                <div class="card twitter d-none d-md-block">
                  <img src="assets/ic-twitter-on.png"
                       srcset="assets/ic-twitter-on@2x.png 2x,
                             assets/ic-twitter-on@3x.png 3x"
                       class="ic-twitter-on">
                  <img class="card-img-top" src="/assets/mcrib.jpg" alt="Card image cap">
                  <div class="card-body">
                    <h4 class="card-title">@McDonalds</h4>
                    <p class="card-text regular">When good things aren't meant to last, you get them while you can. <span class="hashtag">#WednesddayWisdom</span> At part. McDs for a limited time.</p>
                  </div>
                </div>
              </div>
              
              <div id="carousel-tw" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                  <li data-target="#carousel-tw" data-slide-to="0" class="active"></li>
                  <li data-target="#carousel-tw" data-slide-to="1"></li>
                  <li data-target="#carousel-tw" data-slide-to="2"></li>
                </ol>
                <div class="carousel-inner" role="listbox">
                  <div class="carousel-item active">
                    <div class="card twitter">
                      <img src="assets/ic-twitter-on.png"
                       srcset="assets/ic-twitter-on@2x.png 2x,
                             assets/ic-twitter-on@3x.png 3x"
                       class="ic-twitter-on">
                      <img class="card-img-top" src="/assets/mcrib.jpg" alt="Card image cap">
                      <div class="card-body">
                        <h4 class="card-title">@McDonalds</h4>
                        <p class="card-text regular">When good things aren't meant to last, you get them while you can. <span class="hashtag">#WednesddayWisdom</span> At part. McDs for a limited time.</p>
                      </div>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <div class="card twitter">
                      <img src="assets/ic-twitter-on.png"
                           srcset="assets/ic-twitter-on@2x.png 2x,
                                 assets/ic-twitter-on@3x.png 3x"
                           class="ic-twitter-on">
                      <img class="card-img-top" src="/assets/kitchen.jpg" alt="Card image cap">
                      <div class="card-body">
                        <h4 class="card-title">@McDonalds</h4>
                        <p class="card-text regular">Our annual <span class="hashtag">@RMHC</span> fundraiser may be over but you can still donate your time. RMHC is always looking for helping hands. rmhc.org/chapter-search</p>
                      </div>
                    </div>
                  </div>
                  <div class="carousel-item">
                    <div class="card twitter">
                      <img src="assets/ic-twitter-on.png"
                           srcset="assets/ic-twitter-on@2x.png 2x,
                                 assets/ic-twitter-on@3x.png 3x"
                           class="ic-twitter-on">
                      <img class="card-img-top" src="/assets/frap.jpg" alt="Card image cap">
                      <div class="card-body">
                        <h4 class="card-title">@McDonalds</h4>
                        <p class="card-text regular">Me: I'm going to start decorating right now.<br>Also me: Not before this <span class="hashtag">#McCafe</span> Peppermint Mocha.</p>
                      </div>
                    </div>
                  </div>
                </div>
                <a class="carousel-control-prev" href="#carousel-tw" role="button" data-slide="prev">
                  <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                  <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carousel-tw" role="button" data-slide="next">
                  <span class="carousel-control-next-icon" aria-hidden="true"></span>
                  <span class="sr-only">Next</span>
                </a>
              </div>
              
              <div class="row no-gutters ad-2-10 mcdapp">
                <div class="col-12 col-md-2 logo">
                  <img src="assets/ic-mcd@2x.png" id="ie10up">
                </div>
                <div class="col-12 col-md-10 content-wrapper">
                  <div class="content">
                    <h2>Deals on the download</h2>
                    <a class="btn" href="https://www.mcdonalds.com/us/en-us/mobile-order-and-pay.html">DOWNLOAD THE APP</a>
                  </div>
                </div>
                <div class="overlay"></div>
              </div>

            </div>
          </div>
        </div>
      </div>

      <!--Footer with open block field -->
      <footer class="site-footer">
        <!-- footer navigation -->
        <squarespace:navigation navigationId="footerNav" template="footer-navigation" />
      </footer>

    </div>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"
      integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
      crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"
      integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
      crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta/js/bootstrap.min.js"
      integrity="sha384-h0AbiXch4ZDo7tp9hKZ4TsHbi047NrKGLO3SEJAg45jXxnGIfYzk4Si90RDIqNm1"
      crossorigin="anonymous"></script>
    <script src="/scripts/jquery.bcSwipe.js"></script>
    <script>
      $('#carousel-ig').bcSwipe({ threshold: 20 });
      $('#carouselExampleIndicators').bcSwipe({ threshold: 20 });
      $('#carousel-tw').bcSwipe({ threshold: 20 });
    </script>
  </body>
</html>
